<Window x:Class="DDBB_clase.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DDBB_clase"
        mc:Ignorable="d"
         Title="MiniApp" Height="350" Width="525">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button Margin="20" Content="Cargar Turismo" Click="CargarClientes_Click" />
            <Button Margin="20" Content="Cargar Peliculas" Click="CargarPeliculas_Click" />
        </StackPanel>
        <DataGrid x:Name="dataGridClientes" AutoGenerateColumns="True" />
    </StackPanel>
</Window>